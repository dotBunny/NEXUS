{
  "problemMatcher": [
    {
      "owner": "RunUAT_Error",
      "severity": "error",
      "pattern": [{"regexp": ".*Error:.*"}]
    },
    {
      "owner": "RunUAT_Warning",
      "severity": "warning",
      "pattern": [{ "regexp": ".*Warning:.*"}]
    },
    {
      "owner": "RunUAT_Info",
      "severity": "info",
      "pattern": [{ "regexp": ".*error\\(s\\).*"}]
    }
  ]
}
